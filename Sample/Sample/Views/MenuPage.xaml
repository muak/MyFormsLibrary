    xmlns:cr="clr-namespace:MyFormsLibrary.CustomRenderers;assembly=MyFormsLibrary"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="Sample.Views.MenuPage">
    
